Program programa;
var
  num1, num2 : integer;
begin
  write('Ingrese el primer numero: ');
  readln(num1);
  write('Ingrese el segundo numero: ');
  readln(num2);
  writeln('La suma entre num1 y num2 es: ', num1+num2);
  writeln('Presionar cualquier tecla para cerrar el programa ...');
  readln;
end.
